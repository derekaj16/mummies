@{
    ViewData["Title"] = "Burial Form";
}

@model Burialmain

<div class="text-center">
    <h1 class="display-4">Burial</h1>
</div>

<br />

<div>
    <form class="customForm" method="post">
        <label asp-for="Id">Id:</label>
        <input asp-for="Id" />
        <label asp-for="Northsouth">Northsouth:</label>
        <input asp-for="Northsouth" placeholder="N/S" />
        <label asp-for="Squarenorthsouth">SquareNorthsouth:</label>
        <input type="number" asp-for="Squarenorthsouth" placeholder="190" />
        <label asp-for="Eastwest">Eastwest:</label>
        <input asp-for="Eastwest" placeholder="E/W" />
        <label asp-for="Squareeastwest">Squareeastwest:</label>
        <input type="number" asp-for="Squareeastwest" placeholder="130" />
        <label asp-for="Area">Area:</label>
        <input asp-for="Area" placeholder="NE" />
        <label asp-for="Burialnumber">Burial Number:</label>
        <input type="number" asp-for="Burialnumber" placeholder="1" />
        <label asp-for="Sex">Sex:</label>
        <input asp-for="Sex" placeholder="M/F" />
        <label asp-for="Ageatdeath">Age at Death:</label>
        <input asp-for="Ageatdeath" placeholder="A/C" />
        <label asp-for="Headdirection">Head Direction:</label>
        <input asp-for="Headdirection" placeholder="A/C" />
        <label asp-for="Depth">Depth:</label>
        <input type="number" step=".01" asp-for="Depth" placeholder=".55" />
        <label asp-for="Haircolor">Hair Color:</label>
        <input asp-for="Haircolor" placeholder="B" />

        <button class="button" type="submit">Save</button>
    </form>
</div>
